{
  "name": "skew-vscode",
  "description": "A language service for Skew",
  "version": "0.7.51",
  "publisher": "evanw",
  "repository": {
    "type": "git",
    "url": "https://github.com/evanw/skew-vscode"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:skew"
  ],
  "scripts": {
    "postinstall": "npm_package_engines_vscode=1.41.0 node ./node_modules/vscode/bin/install",
    "vscode:prepublish": "npx tsc"
  },
  "main": "./src/extension",
  "contributes": {
    "languages": [
      {
        "id": "skew",
        "aliases": [
          "Skew",
          "skew"
        ],
        "extensions": [
          ".sk"
        ],
        "icon": {
          "light": "./icon.png",
          "dark": "./icon.png"
        },
        "configuration": "./skew.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "skew",
        "scopeName": "source.skew",
        "path": "./syntaxes/Skew.tmLanguage"
      }
    ]
  },
  "dependencies": {
    "skew": "0.9.13",
    "vscode-languageclient": "5.2.1",
    "vscode-languageserver": "5.2.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3",
    "vscode": "1.1.36"
  }
}
